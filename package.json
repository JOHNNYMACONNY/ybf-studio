    "@babel/preset-typescript": "^7.27.1",
    "@next/bundle-analyzer": "^15.4.5",
    "@playwright/test": "^1.40.0",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^30.0.0",
    "@types/node": "latest",
    "@types/nodemailer": "^7.0.1",
