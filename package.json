    "@tiptap/pm": "^3.0.9",
    "@tiptap/react": "^3.0.9",
    "@tiptap/starter-kit": "^3.0.9",
    "@upstash/context7-mcp": "^1.0.14",
    "autoprefixer": "^10.4.21",
    "cookies-next": "6.1.0",
    "critters": "^0.0.23",
    "lucide-react": "0.533.0",
    "micro": "^10.0.1",
    "next": "^15.4.5",
