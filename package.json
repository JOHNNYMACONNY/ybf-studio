{
  "name": "audioserviceapp",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "db:upsert-services": "node scripts/upsert-services.js",
    "dev": "next dev",
    "dev:clean": "rm -rf .next && next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:production": "node scripts/production-testing.js",
    "test:all": "npm run test:production",
    "test:performance": "npm run test:web-vitals && npm run analyze",
    "test:web-vitals": "npx lighthouse http://localhost:3000 --output=json --output-path=./lighthouse-report.json",
    "test:accessibility": "npx axe-core --exit --dir http://localhost:3000",
    "test:browser": "npx playwright test --browser=chromium,firefox,webkit",
    "test:mobile": "npx playwright test --device=\"iPhone 12\"",
    "test:security": "npm audit --audit-level=moderate",
    "test:build": "npx tsc --noEmit --skipLibCheck && npm run build",
    "test:consistency": "node scripts/test-consistency.js",
    "test:simple": "node scripts/simple-consistency-test.js",
    "analyze": "npx @next/bundle-analyzer",
    "test:api": "echo 'API testing placeholder'",
    "test:database": "echo 'Database testing placeholder'",
    "test:payment": "echo 'Payment testing placeholder'",
    "test:email": "echo 'Email testing placeholder'",
    "test:auth": "echo 'Authentication testing placeholder'",
    "test:features": "echo 'Feature detection testing placeholder'",
    "test:webhook": "node scripts/test-webhook.js http://localhost:3000/api/stripe",
    "test:supabase": "node scripts/test-supabase.js",
    "r2:upload": "node scripts/r2-upload-and-link.js",
    "setup:production": "node scripts/setup-production.js",
    "setup:database": "node scripts/setup-database.js",
    "setup:monitoring": "node scripts/setup-monitoring.js",
    "deploy:production": "node scripts/deploy-production.js",
    "deploy:checklist": "node scripts/deploy-checklist.js",
    "db:migrate": "node scripts/setup-database.js",
    "monitor:setup": "node scripts/setup-monitoring.js",
    "production:prepare": "npm run setup:production && npm run setup:database && npm run setup:monitoring",
    "production:deploy": "npm run build && npm run deploy:production",
    "production:full": "npm run production:prepare && npm run production:deploy",
    "beats:backfill-licenses": "node scripts/backfill-beat-licensetypes.js"
  },
